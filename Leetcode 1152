class Solution:
    def mostVisitedPattern(self, username: List[str], timestamp: List[int], website: List[str]) -> List[str]:
        
        timestamp, username, website = (t for t in zip(*sorted(zip(timestamp, username, website))))
        
        user2web = defaultdict(list)
        n = len(username)
        for i in range(n):
            user2web[username[i]].append(website[i])
        
        web2count= Counter()
        for user, web in user2web.items():
            web2count.update(Counter(set(combinations(web,3))))
            
        
        return sorted(web2count, key=lambda k:(-web2count[k], k))[0]
